@model MemberPortal.Models.Refill

@{
    ViewData["Title"] = "RefillStatus";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    h4 {
        font-family: 'Times New Roman', Times, serif
    }
</style>
<h4>Refill Status</h4>

<div>

    <hr />

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RefillOrderId)
        </dt>
        <dd class="col-sm-10">
            @Model.RefillOrderId
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Subscription_ID)
        </dt>
        <dd class="col-sm-10">
            @Model.Subscription_ID
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DrugID)
        </dt>
        <dd class="col-sm-10">
            @Model.DrugID
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RefillDate)
        </dt>
        <dd class="col-sm-10">
            @Model.RefillDate
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NextRefillDate)
        </dt>
        <dd class="col-sm-10">
            @Model.NextRefillDate
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class="col-sm-10">
            @Model.Status
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FromDate)
        </dt>
        <dd class="col-sm-10">
            @Model.FromDate
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Policy_ID)
        </dt>
        <dd class="col-sm-10">
            @Model.Policy_ID
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Member_ID)
        </dt>
        <dd class="col-sm-10">
            @Model.Member_ID
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Location)
        </dt>
        <dd class="col-sm-10">
            @Model.Location
        </dd>
    </dl>

</div>