@model MemberPortal.Models.Drug

@{
    ViewData["Title"] = "DrugDetailsname";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    h2 {
        font-family: 'Times New Roman', Times, serif
    }
</style>
<h2>Availability of Drug</h2>
<hr />

<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DrugId)
        </dt>
        <dd class="col-sm-10">
            @Model.DrugId
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Manufacturer)
        </dt>
        <dd class="col-sm-10">
            @Model.Manufacturer
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ManufacturedDate)
        </dt>
        <dd class="col-sm-10">
            @Model.ManufacturedDate
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ExpiryDate)
        </dt>
        <dd class="col-sm-10">
            @Model.ExpiryDate
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.cost)
        </dt>
        <dd class="col-sm-10">
            @Model.cost
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UnitPackage)
        </dt>
        <dd class="col-sm-10">
            @Model.UnitPackage
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Quantity)
        </dt>
        <dd class="col-sm-10">
            @Model.Quantity
        </dd>
    </dl>
</div>
<div>

</div>